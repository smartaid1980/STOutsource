<ClosedTalk>
	<Version>1.0</Version>
	<!-- 發那科 i-series with Ethernet -->
	<Register>
		<Device>
			<ID>FANUC</ID>
			<Driver>
				<File>.\CustomDrivers\FocasLib.dll</File>
				<AssemblyName>FocasLib.FOCASDriver</AssemblyName>
				<Parameters>
					<Parameter name="IP" value="192.168.1.202"/>
					<Parameter name="PORT" value="8193"/>
					<Parameter name="SCAN" value ="true" readonly="true"/>
				</Parameters>
			</Driver>
		</Device>
		<Groups>
			<Group>CNC</Group>
		</Groups>
	</Register>
	<!-- 三菱 M70/M70 -->
	<!-- MitsubishiDriver -->
	<!-- SYSTEM_TYPE: 5-M700L, 6-M700M,, 8-M800L, 9-M800M; L-車床, M-銑床 -->
	<!-- MACHINE_NO: 1~255 -->
	<Register>
		<Device>
			<ID>Mitsubishi</ID>
			<Driver>
				<File>.\CustomDrivers\MitsubishiDriver.dll</File>
				<AssemblyName>Mitsubishi.MitsubishiDriver</AssemblyName>
				<Parameters>
					<Parameter name="IP" value="192.168.130.170"/>
					<Parameter name="PORT" value="683"/>
					<Parameter name="SYSTEM_TYPE" value="6"/>
					<Parameter name="MACHINE_NO" value="1"/>
					<Parameter name="TIMEOUT_100MS" value="100"/>
				</Parameters>
			</Driver>
		</Device>
		<Groups>
			<Group>CNC</Group>
		</Groups>
    </Register>
